{
    "manifest_version": 3,
    "name": "Unofficial Paragon Rels Helper",
    "description": "This extension is an unofficial helper extension built to make using the Paragon Rels platform easier.",
    "version": "1.0.0",
    "icons": {
        "16": "icons/paragonrels_helper_logo_16_x_16.png",
        "32": "icons/paragonrels_helper_logo_32_x_32.png",
        "48": "icons/paragonrels_helper_logo_48_x_48.png",
        "128": "icons/paragonrels_helper_logo_128_x_128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/main.js"
            ],
            "matches": [
                "https://*.paragonrels.com/*"
            ],
            "run_at": "document_end"
        }
    ]
}